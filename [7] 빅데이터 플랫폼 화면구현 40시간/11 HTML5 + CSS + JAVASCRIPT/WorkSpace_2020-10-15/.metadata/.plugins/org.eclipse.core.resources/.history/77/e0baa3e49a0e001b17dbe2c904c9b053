
public class test {
	public static void main(String[] args) {
		startOz("ounce");
		System.out.println(startOz("ounce"));
	}

	public static String startOz(String str) {
		String result = "";
		if (str.length() > 4) {
			result = str.substring(0, 2);
		} else if (str.length() == 4) {
			result = str.substring(1, 2);
		} else if (str.length() < 4) {
			result = str.substring(0, 1);
		}
		return result;
	}
}
